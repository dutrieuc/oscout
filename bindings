# FZF bound actions
enter:execute(bat {1} --pager="less +{2}")+accept
change:reload(reload_command {q};)
alt-p:execute-silent(toggle py type;)+reload(reload_command {q};)
alt-x:execute-silent(toggle xml type;)+reload(reload_command {q};)
alt-j:execute-silent(toggle js type;)+reload(reload_command {q};)
alt-D:execute-silent(toggle def prefix;)+reload(reload_command {q};)
alt-C:execute-silent(toggle class prefix;)+reload(reload_command {q};)
alt-N:execute-silent(toggle name prefix;)+reload(reload_command {q};)
alt-I:execute-silent(toggle 'inherits?' prefix;)+reload(reload_command {q};)
